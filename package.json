{
  "name": "ts-tiny-path",
  "version": "0.1.1",
  "description": "A lightweight TypeScript routing library with callable route objects",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc && mv dist/index.js dist/index.cjs && tsc --module esnext && mv dist/index.js dist/index.esm.js",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "routing",
    "routes",
    "router",
    "url",
    "path",
    "typescript",
    "ts",
    "callable",
    "lightweight",
    "tiny",
    "web",
    "framework",
    "navigation",
    "match",
    "middleware",
    "hono",
    "express",
    "pattern",
    "lightweight"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrksye/ts-tiny-path.git"
  },
  "homepage": "https://github.com/mrksye/ts-tiny-path#readme",
  "bugs": {
    "url": "https://github.com/mrksye/ts-tiny-path/issues"
  },
  "devDependencies": {
    "typescript": "^4.1.0"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  }
}
